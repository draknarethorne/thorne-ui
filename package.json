{
  "name": "thorne-ui-tools",
  "private": true,
  "devDependencies": {
    "cspell": "^8.17.3",
    "markdownlint-cli2": "^0.14.0",
    "pyright": "^1.1.390"
  },
  "scripts": {
    "lint:md": "markdownlint-cli2 --config .markdownlint.json \"**/*.md\"",
    "spell": "cspell lint --config .cspell.json \"**/*.{md,txt,xml,py}\"",
    "typecheck": "pyright"
  }
}
